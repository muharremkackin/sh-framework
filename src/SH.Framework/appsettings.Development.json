{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Authentication": {
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
    "ValidateIssuerSigningKey": true,
    "ValidIssuers": ["SH.Framework"],
    "ValidAudiences": ["SH.Framework", "SH.Framework.Api", "SH.Framework.Web"],
    "Expiration": "1d",
    "NotBefore": "0s"
  }
}
